<td>
  <input type="text" value={{contact.name}} oninput={{action (mut contact.name) value="target.value"}}>
</td>
<td>
  <input type="number" value={{contact.age}} oninput={{action (mut contact.age) value="target.value"}}>
</td>
<td>
  {{contact.isNew}}
</td>
<td>
  <b>{{contact.isSaving}}</b>
</td>
<td>
  <b style="color: red;">{{dirtyAttributes}}</b>
</td>
<td><button onclick={{action save attrs.contact}}>Save</button></td>

